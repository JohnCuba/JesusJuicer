{
  "name": "jesus_juicer",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --host",
    "build": "bunx --bun vite build",
    "type-check": "bunx --bun vue-tsc --build",
    "lint": "bunx --bun eslint . --fix",
    "format": "bunx --bun prettier --write src/",
    "gen:api": "bun swagger-typescript-api -p ../open_api.schema.yaml -o ./src/data -n config_api.ts --unwrap-response-data"
  },
  "dependencies": {
    "swrv": "^1.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.17",
    "@tsconfig/node22": "^22.0.1",
    "@types/bun": "^1.2.8",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "daisyui": "^5.0.9",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "rollup-plugin-delete": "^2.2.0",
    "rollup-plugin-gzip": "^4.0.1",
    "swagger-typescript-api": "^13.0.28",
    "tailwindcss": "^4.0.17",
    "typescript": "~5.7.3",
    "vite": "^6.2.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.8"
  }
}
